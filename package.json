{
  "name": "tjb",
  "version": "4.20200326145117.0",
  "description": "tjb",
  "main": "index.js",
  "repository": "https://github.com/cxwithyxy/tjb.git",
  "author": "cx",
  "license": "MIT",
  "dependencies": {
    "ElectronPageTentacle": "https://github.com/cxwithyxy/ElectronPageTentacle.git",
    "conf": "^4.0.0",
    "electron_commandline_UI": "https://github.com/cxwithyxy/electron_commandline_UI.git",
    "forin_promise": "https://github.com/cxwithyxy/forin_promise.git",
    "jquery": "^3.4.0",
    "lodash": "^4.17.13",
    "node-schedule": "^1.3.2",
    "p-limit": "^2.2.0",
    "read-pkg-up": "^7.0.1",
    "sleep-promise": "^8.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "@types/node-schedule": "^1.2.3",
    "electron": "15.5.5",
    "electron-builder": "^20.39.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "tsc && electron .",
    "exe": "tsc && electron-builder --win zip",
    "build": "tsc && electron-builder --dir"
  },
  "build": {
    "appId": "tjb",
    "win": {
      "target": "portable"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!app.conf.json",
      "!ui_log.txt",
      "!**/*.ts",
      "!dist",
      "!yarn-error.log",
      "!*.md",
      "!tsconfig.json",
      "!.vscode/",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  }
}
